                <Label x:Name="Label_SC1" Content="Shield Cell #1" Foreground="#FFCE5A05" HorizontalContentAlignment="Right"/>
                <Label x:Name="Label_SC2" Content="Shield Cell #2" Foreground="#FFCE5A05" HorizontalContentAlignment="Right"/>
                <Label x:Name="Label_CH1" Content="Chaff #1" Foreground="#FFCE5A05" HorizontalContentAlignment="Right"/>
                <Label x:Name="Label_CH2" Content="Chaff #2" Foreground="#FFCE5A05" HorizontalContentAlignment="Right"/>
                <Label x:Name="Label_SNCARG" Content="Cargo Scanner" Foreground="#FFCE5A05" HorizontalContentAlignment="Right"/>
