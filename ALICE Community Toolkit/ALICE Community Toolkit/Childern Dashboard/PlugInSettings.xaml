                <Label x:Name="Label_SNWAKE" Content="Wake Scanner" Foreground="#FFCE5A05" HorizontalContentAlignment="Right"/>
            </StackPanel>

            <StackPanel x:Name="StackBox_Group" HorizontalAlignment="Left" Width="90" Margin="0,0,0,21">
